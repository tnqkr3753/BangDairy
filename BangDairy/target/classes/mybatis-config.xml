<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

<settings>
	<setting name="mapUnderscoreToCamelCase" value="true"/>
	<setting name="jdbcTypeForNull" value="NULL" />
</settings>   

<typeAliases>
	<typeAlias alias="movie" type="com.kosmo.bangdairy.vo.MovieVO"/>
	<typeAlias alias="actor" type="com.kosmo.bangdairy.vo.ActorVO"/>
	<typeAlias alias="genre" type="com.kosmo.bangdairy.vo.GenreVO"/>
	<typeAlias alias="director" type="com.kosmo.bangdairy.vo.DirectorVO"/>
	<typeAlias alias="still" type="com.kosmo.bangdairy.vo.StillVO"/>
	<typeAlias alias="comment" type="com.kosmo.bangdairy.vo.CommentVO"/>
	<typeAlias alias="review" type="com.kosmo.bangdairy.vo.ReviewVO"/>
	<!-- 2020-07-23 이경호 수정
		수정내용:AccountFormVO 추가 -->
	<typeAlias alias="account" type="com.kosmo.bangdairy.vo.AccountFormVO"/>
</typeAliases>

</configuration>   
 
     